{
  "name": "@snyk/registry",
  "description": "Registry",
  "main": "dist/lib/index.js",
  "scripts": {
    "check-only": "! grep 'test.only' $(find test -name *.test.js) -n",
    "test": "npm run test:static-analysis && npm run test:functional",
    "test:static-analysis": "npm run lint && npm run check-only && npm run find-circular",
    "test:functional": "./tap test/functional --jobs=8 -Rdot --timeout=120 && npm run test:apib",
    "test:system": "./tap test/system --jobs=8 -Rdot --timeout=150",
    "test:apib": "node ./scripts/api-validation",
    "coverage": "./tap test --cov --coverage-report=lcov",
    "html-coverage": "npm run check-only && npm run lint && CI=1 ./tap test --cov -Rdot --timeout=60 --coverage-report=html",
    "lint": "npm run eslint && npm run tslint",
    "eslint": "eslint .",
    "tslint": "tslint --project tsconfig.json --format stylish --exclude **/src/**/*.js",
    "check-links": "broken-link-checker http://localhost:8000 -rog",
    "find-circular": "madge --circular ./src",
    "compare": "backstop reference && backstop test",
    "e2e": "npm run build-assets && ts-node --project=test/tsconfig.json node_modules/nightwatch/bin/runner.js --config test/e2e/config.js",
    "e2e-remote": "WEBDRIVER_PORT=9515 ts-node --project=test/tsconfig.json node_modules/nightwatch/bin/runner.js --config test/e2e/config.js --env remote",
    "watch-resources-js": "nodemon -V --watch public/js --watch views -i public/js/build/ -e js,html -x 'npm run compile-js'",
    "watch-webpack-bundle": "webpack --config webpack.config.js --mode development --watch",
    "watch-resources": "parallelshell 'npm run watch-resources-js' 'npm run watch-webpack-bundle'",
    "watch-ts": "tsc --project tsconfig.json -w",
    "snyk-ui-css": "cp -r node_modules/@snyk/snyk-ui/build/assets/. public/style/build/",
    "build-hbs": "handlebars views/partials/alert.html views/partials/loading.html views/partials/limit-list-link.html node_modules/@snyk/snyk-ui/build/components/@chart-tooltip.html -e html -f public/js/build/templates.js",
    "raven-js": "cp -r node_modules/raven-js/dist/raven.min.js public/js/build/",
    "compile-app-js": "{ xargs cat < .frontend ; } > ./public/js/build/app.js",
    "compile-js": "npm run build-hbs && npm run compile-app-js && npm run raven-js",
    "minify-app-js": "uglifyjs public/js/build/app.js --source-map --output public/js/build/app.min.js",
    "minify-js": "npm run minify-app-js",
    "build-css": "mkdir -p public/style/build && npm run snyk-ui-css",
    "build-js": "mkdir -p public/js/build && npm run compile-js && npm run minify-js",
    "build-webpack-bundle": "webpack --config webpack.config.js -p --env.NODE_ENV=production",
    "build-assets": "npm run clean:before && npm run build-css && npm run build-js && npm run build-webpack-bundle && npm run clean:after && npm run md5ify",
    "build-ts": "tsc --project tsconfig.json",
    "build": "npm run build-assets && npm run build-ts",
    "md5ify": "./scripts/md5ify.sh public/style/build/styles/*.css public/js/build/*.js public/js/vendor/*.js > ./deployed-assets.json",
    "clean:before": "if [ -d ./static-assets ]; then find static-assets -type f -delete; fi",
    "clean:after": "rm -f public/style/build/*.md5.css public/js/build/*.md5.js public/js/*.md5.js public/js/vendor/*.md5.js",
    "semantic-release": "semantic-release pre && semantic-release post",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect",
    "start": "bin/start",
    "dev": "DEBUG=snyk:*,-snyk:vuln-detail tsc-watch --project tsconfig.json --onSuccess 'node --inspect .' | ./node_modules/.bin/bunyan",
    "git-lint": "./.reset-git-hooks",
    "diff-lint": "git diff-index --name-only HEAD | grep -E '.js$' | xargs eslint --quiet && git diff-index --name-only HEAD | grep -E '.ts$' | xargs ./node_modules/.bin/tslint",
    "postinstall": "./.reset-git-hooks"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/snyk/registry.git"
  },
  "author": "snyk.io",
  "license": "private",
  "bugs": {
    "url": "https://github.com/snyk/registry/issues"
  },
  "homepage": "https://github.com/snyk/registry",
  "dependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@types/lodash": "^4.14.117",
    "@types/node": "^10.12.0",
