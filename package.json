    "express-xml-bodyparser": "^0.3.0",
    "feature-gateway": "0.0.4",
    "format-link-header": "^2.1.0",
    "handlebars": "^4.0.12",
    "highlightjs": "^9.12.0",
    "honeycomb-beeline": "^1.1.2",
    "hosted-git-info": "^2.7.1",
    "http-errors": "^1.7.1",
    "http-status-codes": "^1.3.0",
    "intercom-client": "^2.10.3",
    "ioredis": "^4.2.0",
